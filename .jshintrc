{
    "debug": true,
    "node": true,
    "browser": true,
    "es5": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 0,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "predef": [
        "$",
        "_",
        "angular",
        "TOOLS",
        "describe",
        "beforeEach",
        "inject",
        "it",
        "expect",
        "testutils"
    ],
    "quotmark": "double",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "globals": {
        "angular": false
    }
}